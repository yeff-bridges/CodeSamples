    "def OneHot(word):\n",
    "    word = '{' + word + '|'\n",
    "    a = ord('a')\n",
    "    codes = list(map(lambda x: ord(x) - a, word))\n",
    "    encoding = torch.zeros([len(word), 28], device='cuda:0')\n",
    "    encoding[range(len(word)),codes] += 1\n",
    "        \n",
    "    return encoding"
   ]
